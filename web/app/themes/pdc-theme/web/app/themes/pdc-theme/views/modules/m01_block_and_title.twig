{# Module M01 - Block and Title #}

<section class="m01-block-and-title py-12">
    <div class="container mx-auto px-4">
        {% if title %}
            <h2 class="text-3xl font-bold mb-6">{{ title }}</h2>
        {% endif %}

        {% if content %}
            <div class="prose max-w-none">
                {{ content|raw }}
            </div>
        {% endif %}
    </div>
</section>
